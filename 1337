	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		dragging = true
	end
end)
uis.InputEnded:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		dragging = false
	end
end)

uis.InputChanged:Connect(function(input)
	if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
		local mousePos = uis:GetMouseLocation().X
		local relative = math.clamp(mousePos - SliderBack.AbsolutePosition.X, 0, SliderBack.AbsoluteSize.X)
		Slider.Size = UDim2.new(0, relative, 1, 0)
		recoilStrength = math.floor((relative / SliderBack.AbsoluteSize.X) * 100)
	end
end)

ResetBtn.MouseButton1Click:Connect(function()
	recoilStrength = 0
	Slider.Size = UDim2.new(0, 0, 1, 0)
end)

