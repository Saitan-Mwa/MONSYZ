local UserInputService = game:GetService("UserInputService")
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local TweenService = game:GetService("TweenService")

-- Basic state
local allButtons = {}        -- store all category buttons for color updates
local categoryFrames = {}    -- store category frames if needed
local selectedColor = Color3.fromRGB(0, 0, 0) -- changed to black
local toggleKey = nil        -- KeyCode to toggle UI
local capturingKey = false
local draggingSlider = false
local sliderMaxTransparency = 0.8 -- map slider 0..1 to 0..0.8 transparency
local tabPanels = {}        -- store panels for each category
local clickSound = Instance.new("Sound")
clickSound.SoundId = "rbxassetid://9118823101" -- Example click sound
clickSound.Parent = game.SoundService

-- ScreenGui parent
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "ExecutorUI"
ScreenGui.Parent = game.CoreGui

-- Minimize Icon
local isMinimized = false
local minimizeIcon = Instance.new("ImageButton")
minimizeIcon.Name = "MinimizeIcon"
minimizeIcon.Size = UDim2.new(0, 50, 0, 50)
minimizeIcon.Position = UDim2.new(0, 50, 0, 50)
minimizeIcon.BackgroundTransparency = 1
minimizeIcon.Image = "rbxassetid://135468920313973"  -- Executor-like icon (gear/code theme)
minimizeIcon.Parent = ScreenGui
minimizeIcon.Visible = false

-- Dragging variables for icon
local draggingIcon = false
local dragStart = nil
local startPos = nil

-- Icon drag logic
minimizeIcon.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        draggingIcon = true
        dragStart = input.Position
        startPos = minimizeIcon.Position
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if draggingIcon and input.UserInputType == Enum.UserInputType.MouseMovement then
        local delta = input.Position - dragStart
        minimizeIcon.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
end)

UserInputService.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        draggingIcon = false
    end
end)

-- Main Frame (root)
local MainFrame = Instance.new("Frame")
MainFrame.Size = UDim2.new(0, 700, 0, 400)
MainFrame.Position = UDim2.new(0.5, -350, 0.5, -200)
MainFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
MainFrame.BorderSizePixel = 0
MainFrame.Parent = ScreenGui

local MainCorner = Instance.new("UICorner", MainFrame)
MainCorner.CornerRadius = UDim.new(0, 10)

-- Title Label
local titleLabel = Instance.new("TextLabel")
titleLabel.Size = UDim2.new(1, 0, 0, 40)
titleLabel.Position = UDim2.new(0, 0, 0, 0)
titleLabel.BackgroundTransparency = 1
titleLabel.Text = "MONSY MAIN CHEAT"
titleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
titleLabel.Font = Enum.Font.GothamBold
titleLabel.TextSize = 20
titleLabel.TextScaled = true
titleLabel.Parent = MainFrame

-- Minimize Button (X-like close at top right)
local minimizeButton = Instance.new("TextButton")
minimizeButton.Name = "MinimizeButton"
minimizeButton.Size = UDim2.new(0, 30, 0, 30)
minimizeButton.Position = UDim2.new(1, -35, 0, 5) -- Moved to the right edge
minimizeButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0) -- Changed to black
minimizeButton.Text = "−"  -- Horizontal line for minimize
minimizeButton.TextColor3 = Color3.new(1, 1, 1) -- White text
minimizeButton.Font = Enum.Font.GothamBold
minimizeButton.TextSize = 20
minimizeButton.Parent = MainFrame
local minBtnCorner = Instance.new("UICorner", minimizeButton)
minBtnCorner.CornerRadius = UDim.new(0, 15)

minimizeButton.MouseButton1Click:Connect(function()
    clickSound:Play()
    isMinimized = not isMinimized
    if isMinimized then
        MainFrame.Visible = false
        minimizeIcon.Visible = true
    else
        MainFrame.Visible = true
        minimizeIcon.Visible = false
    end
end)

-- Container for categories (grid)
local ScrollFrame = Instance.new("ScrollingFrame")
ScrollFrame.Name = "CategoriesContainer"
ScrollFrame.Size = UDim2.new(1, -20, 1, -100) -- Adjusted height to account for title and minimize button
ScrollFrame.Position = UDim2.new(0, 10, 0, 50) -- Adjusted position to start below title
ScrollFrame.BackgroundTransparency = 1
ScrollFrame.BorderSizePixel = 0
ScrollFrame.ScrollBarThickness = 6
ScrollFrame.Parent = MainFrame
ScrollFrame.CanvasSize = UDim2.new(0, 0, 0, 0)

local UIGrid = Instance.new("UIGridLayout")
UIGrid.Parent = ScrollFrame
UIGrid.SortOrder = Enum.SortOrder.LayoutOrder
UIGrid.CellSize = UDim2.new(0, 220, 0, 260) -- Height to accommodate buttons
UIGrid.CellPadding = UDim2.new(0, 10, 0, 10)
UIGrid:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
    ScrollFrame.CanvasSize = UDim2.new(0, 0, 0, UIGrid.AbsoluteContentSize.Y + 10)
end)

-- Helper to update all button colors (applies to existing buttons)
local function updateAllButtonColors(color)
    for _, btn in ipairs(allButtons) do
        if btn and btn.Parent then
            btn.BackgroundColor3 = color
        end
    end
end

-- Function to create category panel
local function CreateCategoryPanel(title)
    local Frame = Instance.new("Frame")
    Frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    Frame.BorderSizePixel = 0
    Frame.Parent = ScrollFrame
    table.insert(categoryFrames, Frame)

    local Corner = Instance.new("UICorner", Frame)
    Corner.CornerRadius = UDim.new(0, 6)

    local Title = Instance.new("TextLabel")
    Title.Text = title
    Title.Size = UDim2.new(1, 0, 0, 20)
    Title.BackgroundTransparency = 1
    Title.TextColor3 = Color3.fromRGB(255, 255, 255)
    Title.Font = Enum.Font.GothamBold
    Title.TextSize = 14
    Title.Parent = Frame

    local ScrollButtons = Instance.new("ScrollingFrame")
    ScrollButtons.Size = UDim2.new(1, -10, 1, -30)
    ScrollButtons.Position = UDim2.new(0, 5, 0, 25)
    ScrollButtons.BackgroundTransparency = 1
    ScrollButtons.ScrollBarThickness = 6
    ScrollButtons.Parent = Frame
    ScrollButtons.CanvasSize = UDim2.new(0, 0, 0, 0)

    local List = Instance.new("UIListLayout")
    List.Parent = ScrollButtons
    List.Padding = UDim.new(0, 6)
    List.SortOrder = Enum.SortOrder.LayoutOrder

    List:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
        ScrollButtons.CanvasSize = UDim2.new(0, 0, 0, List.AbsoluteContentSize.Y + 8)
    end)

    tabPanels[title] = ScrollButtons
    return Frame
end

-- Generic function to add button to any category
local function addButtonToCategory(categoryTitle, text, callback)
    local panel = tabPanels[categoryTitle]
    if not panel then return end

    local button = Instance.new("TextButton")
    button.Size = UDim2.new(1, -20, 0, 36)
    button.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    button.TextColor3 = Color3.new(1, 1, 1)
    button.Font = Enum.Font.GothamSemibold
    button.TextSize = 14
    button.Text = text
    button.AutoButtonColor = false
    button.Parent = panel
    Instance.new("UICorner", button).CornerRadius = UDim.new(0, 6)
    table.insert(allButtons, button)

    button.MouseEnter:Connect(function()
        TweenService:Create(button, TweenInfo.new(0.2), {
            BackgroundColor3 = Color3.fromRGB(20, 20, 20) -- Slight hover effect
        }):Play()
    end)
    button.MouseLeave:Connect(function()
        TweenService:Create(button, TweenInfo.new(0.2), {
            BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        }):Play()
    end)

    button.MouseButton1Click:Connect(function()
        clickSound:Play()
        callback()
    end)
end

-- Create initial category panels
CreateCategoryPanel("MAIN MENU")
CreateCategoryPanel("ROLEPLAY SPAWN GUN")
