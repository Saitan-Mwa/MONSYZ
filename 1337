GridImageCorner.CornerRadius = UDim.new(0, 6)

local function ShowPage(page)
    ScrollFrame.Visible = false
    SettingsPage.Visible = false
    ExecutorPage.Visible = false
    GridPage.Visible = false
    page.Visible = true
end

HomeBtn.MouseButton1Click:Connect(function()
    ShowPage(ScrollFrame)
end)

GridBtn.MouseButton1Click:Connect(function()
    ShowPage(GridPage)
end)

SettingsBtn.MouseButton1Click:Connect(function()
    ShowPage(SettingsPage)
end)

CodeBtn.MouseButton1Click:Connect(function()
    ShowPage(ExecutorPage)
end)

ShowPage(ScrollFrame)

updateAllButtonColors(selectedColor)

-- Icon click to toggle UI
minimizeIcon.MouseButton1Click:Connect(function()
    clickSound:Play()
    isMinimized = not isMinimized
    if isMinimized then
        MainFrame.Visible = false
        minimizeIcon.Visible = true
    else
        MainFrame.Visible = true
        minimizeIcon.Visible = false
    end
end)

print("[ExecutorUI] Loaded. Use the bottom buttons to switch tabs. Click 'Set Toggle Key' and press a key to set hotkey to toggle UI visibility.")
