-- Carry variables
local carryRemote = ReplicatedStorage:WaitForChild("CarryReplic"):WaitForChild("CarryRemotes"):WaitForChild("CarryRemote")
local carryChoice = ReplicatedStorage:WaitForChild("CarryReplic"):WaitForChild("CarryChoices"):WaitForChild("Couple Hug")

-- Function to handle auto-completing the username
local function autoCompleteUsername(prefix)
    local matchingPlayers = {}
    -- Loop through all players and find names that start with the prefix
    for _, plr in pairs(Players:GetPlayers()) do
        if string.sub(plr.Name:lower(), 1, #prefix) == prefix:lower() then
            table.insert(matchingPlayers, plr.Name)
        end
    end

    -- If only one match is found, return that player
    if #matchingPlayers == 1 then
        return matchingPlayers[1]
    elseif #matchingPlayers > 1 then
        -- If there are multiple matches, pick the first match (you can change this if you want a random choice)
        return matchingPlayers[1]  -- This will pick the first matched name
    end

    return nil
end

-- Carry a specific player
local function carryPlayer(target)
    if target then
        carryRemote:FireServer({
            cmd = "Carry",
            firstPlr = target,
            carrychoicesss = carryChoice
        })
    else
        warn("Player not found.")
    end
end

-- Command functions
local commands = {}

commands["BringAll"] = function()
    for _, plr in pairs(Players:GetPlayers()) do
        if plr ~= player then
            carryRemote:FireServer({
                cmd = "Carry",
                firstPlr = plr,
                carrychoicesss = carryChoice
            })
        end
    end
    wait(0.03)
    carryRemote:FireServer({ cmd = "Declinecarry" })
end

commands["BringRandom"] = function()
    local others = {}
    for _, plr in pairs(Players:GetPlayers()) do
        if plr ~= player then
            table.insert(others, plr)
        end
    end
    if #others > 0 then
        local rand = others[math.random(1, #others)]
        carryRemote:FireServer({
            cmd = "Carry",
            firstPlr = rand,
            carrychoicesss = carryChoice
        })
        wait(0.03)
        carryRemote:FireServer({ cmd = "Declinecarry" })
    end
end

commands["CrashServer"] = function()
    while true do
        for _, plr in pairs(Players:GetPlayers()) do
            if plr ~= player then
                carryRemote:FireServer({
                    cmd = "Carry",
                    firstPlr = plr,
                    carrychoicesss = carryChoice
                })
            end
        end
        carryRemote:FireServer({ cmd = "Declinecarry" })
        task.wait(0.03)  -- Slight delay to prevent immediate crash
    end
end

commands["BringPlayer"] = function(cmdText)
    local fullUsername = autoCompleteUsername(cmdText)
    if fullUsername then
        local targetPlayer = Players:FindFirstChild(fullUsername)
        if targetPlayer then
            carryPlayer(targetPlayer)
        else
            warn("Player not found: " .. fullUsername)
        end
    else
        warn("No matching player found for: " .. cmdText)
    end
end

commands["Secret"] = function()
    local chatService = ReplicatedStorage:WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest")
    chatService:FireServer("MONSY BALABAGKUMANTOT", "All")
end

-- Toggle logic
local isOn = false

toggleButton.MouseButton1Click:Connect(function()
    isOn = not isOn
    if isOn then
        toggleButton.Text = "ON"
        toggleButton.TextColor3 = Color3.fromRGB(0, 255, 0) -- Green ON
        local cmdText = textBox.Text
        local commandFunc = commands[cmdText:match("^[%w_]+")] -- Extract the command part, ignoring spaces

        if commandFunc then
            if cmdText:sub(1, 10) == "BringPlayer" then
                -- For BringPlayer, we need to pass the full command text (username input)
                commandFunc(textBox.Text:sub(12))  -- Substring to skip the "BringPlayer" part
            else
                commandFunc()
            end
        else
            warn("Unknown command: " .. cmdText)
        end
    else
        toggleButton.Text = "OFF"
        toggleButton.TextColor3 = Color3.fromRGB(255, 255, 255) -- White OFF
    end
end)
end)

